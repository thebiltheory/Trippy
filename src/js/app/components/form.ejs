<form id="trip-form" class="form-horizontal">
<fieldset>

<!-- Select Basic -->
<div class="form-group">
  <div class="col-md-5">
    <select id="trip-from" name="selectbasic" class="form-control">
      <option>From</option>
      <% cities.forEach(function(city){ %>
        <option value="<%= city %>"><%= city %></option>
      <% }); %>
    </select>
  </div>
  <div class="col-md-5">
    <select id="trip-to" name="selectbasic" class="form-control">
      <option>To</option>
      <% cities.forEach(function(city){ %>
        <option value="<%= city %>"><%= city %></option>
      <% }); %>
    </select>
  </div>
</div>

<!-- Multiple Radios (inline) -->
<div class="form-group" id="trip-mode">
  <label class="radio-inline" for="cheapest">
    <input type="radio" name="trip-mode" id="cheapest" value="cheapest" checked>
    Cheapest
  </label>
  <label class="radio-inline" for="fastest">
    <input type="radio" name="trip-mode" id="fastest" value="fastest">
    Fastest
  </label>
</div>

<!-- Button -->
<div class="form-group">
  <div class="col-md-4">
    <button id="singlebutton" name="singlebutton" class="btn btn-primary">Let's Go</button>
  </div>
</div>

</fieldset>
</form>
